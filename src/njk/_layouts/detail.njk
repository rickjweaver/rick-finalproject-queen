{% extends '_layouts/base.njk' %}

  {% block content %}

  <div class="container">
  <div class="row">
    <div class="col-sm-4 left-rail">
    <img src="assets/img/members/{{ members.image }}" alt="members" class="img-fluid">
    <p><h3><div style="font-family: 'Tangerine', cursive;">{{ members.name }}</div></h3>
    <cite>{{ members.instruments }}</cite>
    {{ members.tenure }}
    <ul>
    <li>{{ members.birthplace }}</li>
    <li>{{ members.birthdate }}</li>
   </div>

    <div class="col-sm-8 right-rail">
     <p>{{ members.text | safe }}</p>
    </div>
  </div>
</div>

  {% endblock %}


